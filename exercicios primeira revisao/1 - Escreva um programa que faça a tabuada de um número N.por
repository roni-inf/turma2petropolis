programa
{
	
	funcao inicio()
	{
		//1-Escreva um programa que faça a tabuada de um número n.

		inteiro n
		escreva("Digite um numero: ")
		leia(n)
		para(inteiro i=0; i < 10; i++){
			escreva(n,"x",i+1,"= ",(i+1)*n,"   ")	
		}
	}
}
