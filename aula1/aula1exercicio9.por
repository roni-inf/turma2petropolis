programa
{
	
	funcao inicio()
	{
		real distanciaPercorrida, totalGasto, consumoMedio

		escreva("Passe a distância percorrida em Km: ")
		leia(distanciaPercorrida)

		escreva("Passe o total de combustível gasto: ")
		leia(totalGasto)

		consumoMedio = distanciaPercorrida/totalGasto
		escreva("O consumo médio foi de: " + consumoMedio + "Km/L")

		
	}
}
