programa
{
	
	funcao inicio()
	{
		inteiro numero
		escreva("Digite o numero: ")
		leia(numero)

		escreva("Sucessor: ", numero + 1, "\n")
		escreva("Antecessor: ", numero - 1)
		
	}
}
